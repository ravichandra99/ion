{
  "name": "ionic-react-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "yarn install && yarn set:yarn",
    "dev:web": "ionic serve",
    "dev:ios": "ionic cap run ios -l --external",
    "dev:android": "ionic cap run android -l --external",
    "build": "react-scripts build && ionic cap copy",
    "start:ios": "cap open ios",
    "start:android": "cap open android",
    "sync": "ionic cap sync",
    "set:yarn": "ionic config set -g npmClient yarn"
  },
  "dependencies": {
    "@capacitor/android": "^2.0.0",
    "@capacitor/core": "2.0.0",
    "@capacitor/ios": "^2.0.0",
    "@ionic/react": "^5.0.7",
    "@ionic/react-router": "^5.0.7",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "@capacitor/cli": "2.0.0",
    "@ionic/cli": "^6.4.1",
    "@types/react-router-dom": "^5.1.3",
    "rimraf": "^3.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">= 12.x.x",
    "yarn": ">= 1.20",
    "npm": "Please use Yarn"
  }
}